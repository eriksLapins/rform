{
  "name": "rust_cmd_npm",
  "version": "1.0.0",
  "description": "a package is a template for when you would like to create a rust app that can be used as a command line app in node/javascript projects",
  "main": "index.js",
  "author": "Eriks Lapins",
  "license": "MIT",
  "private": true,
  "scripts": {
	  "typecheck": "tsc --noEmit",
	  "lint": "oxlint .",
	  "lint:fix": "oxlint . --fix",
	  "build": "tsc",
	  "dev": "yarn build && node lib/index.js"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "oxlint": "^0.2.8",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
	  "rust_cmd_npm-linux-x64": "1.0.0",
	  "rust_cmd_npm-linux-arm64": "1.0.0",
	  "rust_cmd_npm-darwin-x64": "1.0.0",
	  "rust_cmd_npm-darwin-arm64": "1.0.0",
	  "rust_cmd_npm-windows-x64": "1.0.0",
	  "rust_cmd_npm-windows-arm64": "1.0.0"
  }
}
